
import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
	public static void main (String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int t=sc.nextInt();
	    for(int i=0;i<t;i++)
	    {
	        int n=sc.nextInt();
	        int arr[]=new int[n];
	        for(int j=0;j<n;j++)
	        {
                  arr[j]=sc.nextInt();	            
	        }
	        int k=sc.nextInt();
	        Arrays.sort(arr);
	        System.out.println(arr[k-1]);
	    }
	}
}
