class test
{
    public static void main (String[] args) {
Scanner sc=new Scanner(System.in);
int t=sc.nextInt();
while(t-->0){
String st=sc.next();

int l=st.length();
char a1[]=st.toCharArray();
HashSet<Character> hs=new HashSet<Character>();
for(char i:a1){
hs.add(i);
}
int ans=l-hs.size();
System.out.println(ans);
}
}
}
